<div class="new-link">
<%= simple_form_for @link do |form| %>
  <%= form.input :title, label: "Link title" %>
  <%= form.input :site, label: "The Site" %>
  <%= form.input :url, label: "Story Link" %>
  <%= form.input :description, label: "Description" %>
  <%= form.button :submit %>
<% end %>
</div>
